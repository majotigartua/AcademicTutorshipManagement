<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="academictutorshipmanagement.views.SelectEducationalProgramRoleFXMLController">
   <children>
      <Label layoutX="25.0" layoutY="30.0" prefHeight="40.0" prefWidth="430.0" text="Seleccione un programa educativo y el rol con el que desea ingresar al sistema, y dé clic en el botón de &quot;Aceptar&quot;." wrapText="true">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <Label layoutX="25.0" layoutY="105.0" prefHeight="20.0" prefWidth="150.0" text="Programa educativo:" wrapText="true">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="educationalProgramComboBox" layoutX="180.0" layoutY="102.0" prefHeight="26.0" prefWidth="275.0" style="-fx-font: 14px &quot;Arial&quot;;" />
      <Label layoutX="25.0" layoutY="175.0" prefHeight="20.0" prefWidth="150.0" text="Rol:" wrapText="true">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="roleComboBox" layoutX="75.0" layoutY="172.0" prefHeight="26.0" prefWidth="380.0" style="-fx-font: 14px &quot;Arial&quot;;" />
      <Button layoutX="25.0" layoutY="245.0" mnemonicParsing="false" onAction="#acceptButtonClick" prefHeight="26.0" prefWidth="125.0" text="Aceptar">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Button>
      <Button layoutX="330.0" layoutY="245.0" mnemonicParsing="false" onAction="#cancelButtonClick" prefHeight="20.0" prefWidth="125.0" text="Cancelar">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
